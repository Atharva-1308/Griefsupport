{
  "name": "grief-ai-chatbot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "cd backend && python -m uvicorn main:app --reload --port 8000 --host 0.0.0.0",
    "backend-https": "cd backend && python -m uvicorn main:app --reload --port 8000 --host 0.0.0.0 --ssl-keyfile=certs/key.pem --ssl-certfile=certs/cert.pem",
    "start": "concurrently \"npm run backend\" \"npm run dev\"",
    "start-https": "concurrently \"npm run backend-https\" \"npm run dev\"",
    "start-http": "concurrently \"npm run backend\" \"vite --host 0.0.0.0\"",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "setup": "npm install && npm run install-backend"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1",
    "axios": "^1.6.2",
    "socket.io-client": "^4.7.4",
    "react-hot-toast": "^2.4.1",
    "framer-motion": "^10.16.16",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}